<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.felix.Allergenius">

    <application
        android:debuggable="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.felix.Allergenius.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.felix.Allergenius.SCREEN" />
        <activity android:name="com.example.felix.Allergenius.game" />
        <activity android:name="com.example.felix.Allergenius.firstaid" />
        <activity android:name="com.example.felix.Allergenius.smoothiePopUp" />
        <activity android:name="com.example.felix.Allergenius.questionnaire"></activity>
    </application>

</manifest>
